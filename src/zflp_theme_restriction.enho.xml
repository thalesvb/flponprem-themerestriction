 <asx:abap xmlns:asx="http://www.sap.com/abapxml" version="1.0">
  <asx:values>
   <TOOL>HOOK_IMPL</TOOL>
   <SHORTTEXT>Apply restrictions for SAP Fiori Launchpad</SHORTTEXT>
   <ORIGINAL_OBJECT>
    <PGMID>R3TR</PGMID>
    <ORG_OBJ_TYPE>CLAS</ORG_OBJ_TYPE>
    <ORG_OBJ_NAME>/UI2/CL_EDM_DA_INTEROP_THM_00</ORG_OBJ_NAME>
    <ORG_MAIN_TYPE>CLAS</ORG_MAIN_TYPE>
    <ORG_MAIN_NAME>/UI2/CL_EDM_DA_INTEROP_THM_00</ORG_MAIN_NAME>
    <PROGRAMNAME>/UI2/CL_EDM_DA_INTEROP_THM_00=CP</PROGRAMNAME>
   </ORIGINAL_OBJECT>
   <ENHANCEMENTS>
    <ENH_HOOK_IMPL>
     <PROGRAMNAME>/UI2/CL_EDM_DA_INTEROP_THM_00=CP</PROGRAMNAME>
     <ENHMODE>D</ENHMODE>
     <FULL_NAME>\TY:/UI2/CL_EDM_DA_INTEROP_THM_00\ME:CLASS_CONSTRUCTOR\SE:END\EI</FULL_NAME>
     <SOURCE>
      <item>data theme_restriction type ref to zflp_theme_restriction.</item>
      <item>try.</item>
      <item>get badi theme_restriction.</item>
      <item>call badi theme_restriction-&gt;modify_allowed_themes</item>
      <item>changing themes = mt_theme.</item>
      <item>catch CX_BADI_NOT_IMPLEMENTED.</item>
      <item>endtry.</item>
     </SOURCE>
    </ENH_HOOK_IMPL>
   </ENHANCEMENTS>
   <SPACES>
    <item>
     <FULL_NAME>\TY:/UI2/CL_EDM_DA_INTEROP_THM_00\ME:CLASS_CONSTRUCTOR\SE:END\EI</FULL_NAME>
     <SPACES>
